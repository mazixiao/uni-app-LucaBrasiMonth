{
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/enter/enter",
			"style": {
				"navigationBarTitleText": "进入页"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "我的"
			},
			"usingComponents": {
				"van-toast": "/wxcomponents/vant/dist/Toast/index",
				"van-button": "/wxcomponents/vant/dist/button/index"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/apply/apply",
			"style": {
				"navigationBarTitleText": "apply"
			}
		},
		{
			"path": "pages/applyList/applyList",
			"style": {
				"navigationBarTitleText": "applyList",
				"onReachBottomDistance":100 //距离底部多远时触发 单位px
			}
		}

	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom",
		"app-plus": {
			"titleNView": false
		}
	},

	"tabBar": {
		"color": "#8f9198",
		"selectedColor": "9c528a",
		"backgroundColor": "#fff",
		"borderStyle": "black",
		"list": [{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "/static/images/link-icon1.png",
				"selectedIconPath": "/static/images/link-icon1-hover.png"
			},
			{
				"pagePath": "pages/my/my",
				"text": "我的",
				"iconPath": "/static/images/link-icon2.png",
				"selectedIconPath": "/static/images/link-icon2-hover.png"
			}
		]
	}


}
